{
  "name": "angular2-now",
  "version": "1.0.1",
  "private": true,
  "description": "Angular 2 component syntax for Angular 1 apps",
  "main": "dist/angular2-now.js",
  "scripts": {
    "meteor": "gulp meteor",
    "test": "karma start karma.config.js --set-env-NODE_ENV=ci",
    "test:local": "karma start karma.config.js",
    "build:dist": "webpack -d --progress --colors --set-env-NODE_ENV=development",
    "build:prod": "webpack -d --progress --colors --set-env-NODE_ENV=production",
    "build": "npm run build:dist & npm run build:prod & npm run meteor",
    "coverage:coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pbastowski/angular2-now.git"
  },
  "keywords": [
    "angular",
    "angular1",
    "angular2",
    "annotations",
    "decorators",
    "babel"
  ],
  "author": "Paul Bastowski <pbastowski@gmail.com> (https://github.com/pbastowski)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pbastowski/angular2-now/issues"
  },
  "homepage": "https://github.com/pbastowski/angular2-now",
  "devDependencies": {
    "angular": "^1.4.0",
    "angular-mocks": "^1.4.0",
    "angular-ui-router": "^0.2.15",
    "argv-set-env": "^1.0.0",
    "babel": "^5.8.3",
    "babel-core": "^5.0.0",
    "babel-loader": "^5.3.2",
    "coveralls": "^2.11.4",
    "gulp": "3.9.0",
    "gulp-replace": "0.5.4",
    "isparta": "^4.0.0",
    "isparta-loader": "1.0.0",
    "jasmine-core": "^2.3.4",
    "karma": "0.13.15",
    "karma-babel-preprocessor": "^5.2.2",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coverage": "^0.5.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.6",
    "karma-jasmine-html-reporter": "^0.1.8",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "lodash": "^3.10.1",
    "webpack": "^1.12.9"
  }
}
